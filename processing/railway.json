{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "RAILPACK"
  },
  "deploy": {
    "startCommand": "playwright install --with-deps chromium && python cs2_processor.py",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 1,
    "resources": {
      "cpu": 1
    }

  }
}
